{
    "manifest_version": 2,

    "name": "Vigil",
    "description": "Count of visited sites.",
    "version": "1.0",

    "permissions": [
        "tabs",
        "webNavigation"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "main.html"
    },
    "chrome_url_overrides" : {
    "newtab": "main.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["script/jquery-1.11.1.min.js", "script/func.js"]
    }]
}